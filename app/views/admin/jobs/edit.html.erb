<div class="col-md-4 col-md-offset-4">
  <h2 class="text-center">Edit Job</h2>
  <hr>
  <%= simple_form_for [:admin, @job] do |f| %>

  <%= f.input :title %>
  <%= f.input :description %>
  <%= f.input :wage_lower_bound %>
  <%= f.input :wage_upper_bound %>
  <%= f.input :contact_email %>
  <%= f.input :is_hidden %>
  <%= f.submit "Submit" %>
  <% end %>
</div>
