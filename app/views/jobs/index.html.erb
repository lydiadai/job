<div class="col-md-12">

  <table class="table table-hover">
    <thead>
      <tr>
        <td>#</td>
        <td>Title</td>
        <td>Description</td>
        <td>Wage Lower Bound</td>
        <td>Wage Upper Bound</td>
        <td>Contact Email</td>
        <td>Time</td>
        <td>Creator</td>
      </tr>
    </thead>
    <tbody>

      <%= render :partial => "job_item", :collection => @jobs,:as => :job %>

    </tbody>
  </table>
  <div class="text-center">
    <%= will_paginate @jobs %>
  </div>
</div>
